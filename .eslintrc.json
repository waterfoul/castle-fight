{
  "parser": "babel-eslint",
  "plugins": [
	"flowtype"
  ],
  "extends": [
	"eslint:recommended",
	"plugin:react/recommended",
	"plugin:flowtype/recommended"
  ],
  "env": {
	"node": true
  },
  "rules": {
	"flowtype/require-valid-file-annotation": [
	  2,
	  "always"
	],
	"flowtype/boolean-style": [
	  2,
	  "bool"
	],
	"react/prop-types": 0
  }
}